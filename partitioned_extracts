SELECT *
FROM colorado
WHERE entityformdate < '1900-01-01';

SELECT *
FROM colorado
WHERE entityformdate >= '1900-01-01' and entityformdate < '1950-01-01';

SELECT *
FROM colorado
WHERE entityformdate >= '1950-01-01' and entityformdate < '2000-01-01';

SELECT *
FROM colorado
WHERE entityformdate >= '2000-01-01' and entityformdate < '2010-01-01';

SELECT *
FROM colorado
WHERE entityformdate > '2020-01-01';

select 
    *
from colorado
WHERE entityname LIKE '%Alteryx%';